unit Testunt.front.enderecoController;
{

  Delphi DUnit Test Case
  ----------------------
  This unit contains a skeleton test case class generated by the Test Case Wizard.
  Modify the generated code to correctly setup and call the methods from the unit 
  being tested.

}

interface

uses
  TestFramework, unt.front.endereco.controller, unt.endereco.dto,
  unt.front.endereco.dao;

type
  // Test methods for class TFEnderecoController

  TestTFEnderecoController = class(TTestCase)
  strict private
    FFEnderecoController: TFEnderecoController;
  public
    procedure SetUp; override;
    procedure TearDown; override;
  published
    procedure TestLoad;
  end;

implementation

procedure TestTFEnderecoController.SetUp;
begin
  FFEnderecoController := TFEnderecoController.Create;
end;

procedure TestTFEnderecoController.TearDown;
begin
  FFEnderecoController.Free;
  FFEnderecoController := nil;
end;

procedure TestTFEnderecoController.TestLoad;
var
  ReturnValue: TEnderecoDto;
  ACep: string;
begin
  ACep := '03606-010';
  ReturnValue := FFEnderecoController.Load(ACep);
  CheckNotNull(ReturnValue, 'Erro na funcao load do controller');
end;

initialization
  RegisterTest(TestTFEnderecoController.Suite);
end.

